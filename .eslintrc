{
  "extends": ["airbnb"],
  "rules": {
    "react/prefer-stateless-function": 0,
    "react/jsx-filename-extension": 0,
    "react/jsx-one-expression-per-line": 0,
    "implicit-arrow-linebreak": 0,
    "react/prop-types": 0,
    "arrow-parens": 0,
    "global-require": 0,
    "import/no-extraneous-dependencies": 0,
    "no-param-reassign": 0,
    "default-case": 0,
    "import/prefer-default-export": 0
  },
  "env": {
    "browser": true
  }
}
